  "interactions": [
    {
      "description": "a request for todo list",
      "providerState": "get todo list successfully",
      "request": {
        "method": "GET",
        "path": "/items"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": {
          "data": [
            {
              "id": 1,
              "text": "buy some milk"
            }
          ]
        },
        "matchingRules": {
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          },
          "$.body.data[*].id": {
            "match": "type"
          },
          "$.body.data[*].text": {
            "match": "type"
          }
        }
      }
    },
